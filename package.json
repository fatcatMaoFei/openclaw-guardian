{
  "name": "openclaw-guardian",
  "version": "0.2.1",
  "description": "Security gate plugin for OpenClaw \u2014 two-tier blacklist (regex + LLM intent verification) prevents dangerous tool executions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "openclaw.plugin.json",
    "default-policies.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "security",
    "safety",
    "guardian",
    "blacklist",
    "llm",
    "tool-safety"
  ],
  "author": "fatcatMaoFei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatcatMaoFei/openclaw-guardian.git"
  },
  "bugs": {
    "url": "https://github.com/fatcatMaoFei/openclaw-guardian/issues"
  },
  "homepage": "https://github.com/fatcatMaoFei/openclaw-guardian#readme",
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "typescript": "^5.9.0"
  },
  "openclaw": {
    "extensions": [
      "dist/index.js"
    ]
  }
}